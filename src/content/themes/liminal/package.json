{
    "name": "wanovak.com",
    "description": "A blog",
    "demo": "https://github.com/wanovak/wanovak.github.io",
    "version": "1.0.0",
    "engines": {
        "ghost": ">=3.11.0",
        "ghost-api": "v3"
    },
    "author": {
        "name": "wanovak",
        "url": "https://wanovak.com"
    },
    "config": {
        "posts_per_page": 5,
        "image_sizes": {
            "xs": {
                "width": 400
            },
            "s": {
                "width": 600
            },
            "m": {
                "width": 800
            },
            "l": {
                "width": 1000
            },
            "xl": {
                "width": 3000
            }
        }
    },
    "gpm": {
        "type": "theme",
        "categories": [
            "Minimal",
            "Responsive",
            "Starter theme",
            "Boilerplate",
            "Typographic"
        ]
    },
    "keywords": [
        "responsive",
        "ghost",
        "starter",
        "theme",
        "ghost-theme",
        "boilerplate"
    ],
    "browserslist": [
        "last 2 versions",
        "ie >= 9",
        "android >= 4.4",
        "ios >= 7"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/wanovak/wanovak.github.io"
    },
    "devDependencies": {
        "gulp": "^4.0.2",
        "gulp-autoprefixer": "^7.0.1",
        "gulp-run-command": "0.0.10",
        "gulp-sass": "^4.1.0",
        "merge-stream": "^2.0.0"
    },
    "dependencies": {
        "@fortawesome/fontawesome-free": "^5.13.0",
        "foundation-sites": "^6.6.3",
        "gulp-concat": "^2.6.1",
        "sticky-js": "^1.3.0",
        "tocbot": "^4.11.1"
    }
}
